{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["google", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "new-cap": 0,
        "quotes": [2, "single", { "avoidEscape": true }]
    }
}
